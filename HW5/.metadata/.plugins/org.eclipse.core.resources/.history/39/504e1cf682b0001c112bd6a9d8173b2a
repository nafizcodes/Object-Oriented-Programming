import java.util.Arrays;

public class CruiseShip {
	private String name;
	private String year;
	private String visits[];
	private int max_passengers;
	
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}

	public String getYear() {
		return year;
	}

	public void setYear(String year) {
		this.year = year;
	}

	public String[] getVisits() {
		return visits;
	}

	public void setVisits(String[] visits) {
		this.visits = visits;
	}

	public int getMax_passengers() {
		return max_passengers;
	}

	public void setMax_passengers(int max_passengers) {
		this.max_passengers = max_passengers;
	}
	
	public String describeShip() {
		return toString();
	}

	@Override
	public String toString() {
		return "CruiseShip [name=" + getName() + ", year=" + getYear() + ", visits=" + Arrays.toString(visits)
				+ ", max_passengers=" + getMax_passengers() + "]";
	}
		
}
